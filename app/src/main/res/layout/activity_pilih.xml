<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@drawable/radio"
    android:padding="10dp"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ads"
        android:layout_marginTop="14dp"
        android:layout_below="@+id/adView"
        android:layout_centerHorizontal="true" />


    <Button android:id="@+id/button4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Interistial + Banner auto close"
        android:background="@drawable/radio"
        android:layout_above="@+id/button5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="13dp" />
    <Button android:id="@+id/button5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Interistial + 10 Banner auto click random"
        android:background="@drawable/radio"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <com.google.android.gms.ads.AdView
        android:id="@+id/adView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        ads:adSize="SMART_BANNER"
        ads:adUnitId="a"

        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"

        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true">
    </com.google.android.gms.ads.AdView>
    <Button android:id="@+id/button3"
        android:layout_width="match_parent"
        android:background="@drawable/radio"
        android:layout_height="wrap_content"
        android:text="  Interistial + 1 Banner Pedas  "
        android:layout_above="@+id/button6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="17dp" />
    <Button android:id="@+id/button6" android:background="@drawable/radio" android:visibility="visible" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="  Interistial + 3 Banner with keyword  "
        android:layout_marginBottom="14dp"
        android:layout_above="@+id/button4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <Button android:id="@+id/button7" android:background="@drawable/radio"
        android:visibility="visible" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="      Admob Auto Impresion     "
        android:layout_above="@+id/button3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="16dp" />
    <TextView android:id="@+id/titleText" android:background="@drawable/radio" android:visibility="invisible" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="1" />

    <Button
        android:id="@+id/rew"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:text="Button" />

</RelativeLayout>
